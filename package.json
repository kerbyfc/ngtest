{
  "name": "ngtest",
  "version": "1.0.0",
  "description": "angular test app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rimraf dist/",
    "test": "karma start",
    "test:debug": "DEBUG=true npm test",
    "test:bdd": "BDD=true npm test",
    "test:e2e": "cucumber-tsflow-suite ./tests/e2e",
    "start": "npm run dev",
    "dev": "webpack-dev-server --hot --inline --progress --colors --content-base src --port 9001",
    "prod": "npm run clean && webpack --config=webpack.config.production.js -p",
    "prod:serve": "lite-server -c config/prod-config.json",
    "dogen": "gulp dogen"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kerbyfc/ngtest.git"
  },
  "keywords": [
    "angular",
    "karma",
    "es6",
    "jasmine",
    "cucumber"
  ],
  "author": "Alex Lukin",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "cucumber-tsflow-suite": "git+ssh://git@github.com:kerbyfc/cucumber-tsflow-suite.git#master",
    "gulp": "3.9.1",
    "gulp-dogen": "0.1.9",
    "jasmine-core": "^2.4.1",
    "jasmine-sinon": "^0.4.0",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.1",
    "karma-clear-screen-reporter": "^1.0.0",
    "karma-jasmine": "^0.3.6",
    "karma-json-fixtures-preprocessor": "0.0.5",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "selenium-webdriver": "^2.47.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-local-storage": "^0.2.2",
    "angular-mocks": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-ui-bootstrap": "^1.1.2",
    "angular-ui-router": "^1.0.0-beta.2",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "bootstrap": "^3.3.6",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "es6-shim": "^0.35.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lite-server": "^2.2.2",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "ngtemplate-loader": "^1.3.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
